{
  "name": "rolex-reserve-pro",
  "version": "0.1.0",
  "private": true,
  "main": "dist/electron/main.cjs",
  "type": "commonjs",
  "exports": {
    ".": "./dist/electron/main.cjs"
  },
  "scripts": {
    "// ===== TESTING =====": "",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "// ===== NEXT.JS 웹 앱 =====": "",
    "web:dev": "next dev -p 3000",
    "web:build": "next build",
    "web:start": "next start",
    "web:lint": "next lint",
    "// ===== MAC 환경 =====": "",
    "mac:prepare": "tsc --project tsconfig.electron.json && node scripts/rename-js-to-cjs.cjs || true",
    "mac:copy-assets": "node scripts/copy-utils.cjs && node scripts/copy-js-files.cjs",
    "mac:dev": "npm run mac:prepare && npm run mac:copy-assets && concurrently -k \"npm run web:dev\" \"wait-on http://localhost:3000 && npx electron dist/electron/main.cjs\"",
    "mac:build": "npm run mac:prepare && npm run mac:copy-assets && npm run web:build && electron-builder --mac",
    "// ===== WINDOWS 환경 =====": "",
    "win:prepare": "tsc --project tsconfig.electron.json && (if exist dist\\electron\\main.js (move dist\\electron\\main.js dist\\electron\\main.cjs) else (echo main.js 파일이 없지만 계속 진행합니다.))",
    "win:copy-assets": "node scripts/copy-utils.cjs && node scripts/copy-js-files.cjs",
    "win:dev": "npm run win:prepare && npm run win:copy-assets && concurrently -k \"npm run web:dev\" \"wait-on http://localhost:3000 && npx electron dist\\electron\\main.cjs\"",
    "win:build": "npm run win:prepare && npm run win:copy-assets && npm run web:build && electron-builder --win",
    "win:build-run": "npm run win:build && start release\\Rolex Reserve Pro Setup.exe",
    "// ===== 데이터베이스 =====": "",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx db/seed.ts",
    "// ===== 통합 명령어 =====": "",
    "dev": "npm run mac:dev",
    "dev:win": "npm run win:dev",
    "build": "npm run mac:build",
    "build:win": "npm run win:build",
    "build:all": "npm run mac:build && npm run win:build"
  },
  "bin": {
    "rolex-reserve": "./scripts/startReservation.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@neondatabase/serverless": "^1.0.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-toast": "^1.2.13",
    "@vercel/postgres": "^0.10.0",
    "antd": "^5.25.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^11.1.0",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.43.1",
    "inquirer": "^9.3.7",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "playwright": "^1.52.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/electron-store": "^1.3.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^36.3.0",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "electron-store": "^10.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "fs-extra": "^11.3.0",
    "jest": "^29.7.0",
    "next": "^15.3.2",
    "postcss": "^8.4.35",
    "rimraf": "^6.0.1",
    "sonner": "^1.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3"
  },
  "description": "Rolex Reserve Pro - Rolex Reservation Management System",
  "author": "Your Name <cjy0573@naver.com>",
  "build": {
    "appId": "com.rolexreserve.app",
    "productName": "Rolex Reserve Pro",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "electron/resources/icon.ico"
    },
    "directories": {
      "buildResources": "electron/resources",
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "out/**/*"
    ],
    "asar": true,
    "extraMetadata": {
      "main": "dist/electron/main.cjs"
    }
  }
}
